module Q1_vlg_vec_tst();
// constants                                           
// general purpose registers
reg clock;
reg [2:0] instruction;
reg [2:0] operand1;
reg [2:0] opernad0;
// wires                                               
wire [7:0] result_pin;

wire sampler;                             

// assign statements (if any)                          
Q1 i1 (
// port map - connection between master ports and signals/registers   
	.clock(clock),
	.instruction(instruction),
	.operand1(operand1),
	.opernad0(opernad0),
	.result_pin(result_pin)
);

// clock
always
begin
	clock = 1'b0;
	clock = #25000 1'b1;
	#25000;
end 
// instruction[ 2 ]
initial
begin
	instruction[2] = 1'b1;
end 
// instruction[ 1 ]
initial
begin
	instruction[1] = 1'b0;
end 
// instruction[ 0 ]
initial
begin
	instruction[0] = 1'b0;
end 
// operand1[ 2 ]
initial
begin
	operand1[2] = 1'b0;
end 
// operand1[ 1 ]
initial
begin
	operand1[1] = 1'b0;
end 
// operand1[ 0 ]
initial
begin
	operand1[0] = 1'b0;
end 
// opernad0[ 2 ]
initial
begin
	opernad0[2] = 1'b1;
	opernad0[2] = #400000 1'b0;
end 
// opernad0[ 1 ]
initial
begin
	opernad0[1] = 1'b0;
end 
// opernad0[ 0 ]
initial
begin
	opernad0[0] = 1'b0;
end 

Q1_vlg_sample_tst tb_sample (
	.clock(clock),
	.instruction(instruction),
	.operand1(operand1),
	.opernad0(opernad0),
	.sampler_tx(sampler)
);

Q1_vlg_check_tst tb_out(
	.result_pin(result_pin),
	.sampler_rx(sampler)
);
endmodule

